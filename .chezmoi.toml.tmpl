{{- $recipient := promptStringOnce . "gpg recipient" "gpg recipient" -}}
{{- $gitlab_user_email := promptStringOnce . "gitlab user email" "gitlab user email" -}}

# vim: ft=toml

encryption = "gpg"
[gpg]
recipient = {{ $recipient | quote }}
args = ["--quiet"]

[data]
GIT_USER_NAME = "dumh"
GIT_USER_EMAIL = "duminghui@users.noreply.github.com"
GITLAB_USER_EMAIL = {{ $gitlab_user_email | quote }}


